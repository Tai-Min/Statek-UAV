<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="statek_name" default="statek"/>

    <group ns="$(arg statek_name)">
        <!-- base link to lidar link -->
        <node pkg="tf2_ros" type="static_transform_publisher" name="base_link_to_laser_link" args="-0.025 0 0.15 3.08159265359 0 0 $(arg statek_name)/base_link $(arg statek_name)/laser_link"/>
    </group>

</launch>
