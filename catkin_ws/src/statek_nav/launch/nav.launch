<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="statek_name" default="$(eval open(find('statek_config') + '/name').read())"/>

    <include file="$(find statek_nav)/launch/short_term_nav.launch">
        <arg name="statek_name" value="$(arg statek_name)"/>
    </include>

    <include file="$(find statek_nav)/launch/long_term_nav.launch">
        <arg name="statek_name" value="$(arg statek_name)"/>
    </include>
</launch>
