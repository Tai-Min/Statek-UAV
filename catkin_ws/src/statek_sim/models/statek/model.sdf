    <link name='right_front_wheel'>

      <pose>-0.264648 -0.166853 -0.031261 0 -0 1.5708</pose>

      <visual name='visual'>
        <geometry>
          <mesh>
            <uri>model://statek/wheel.dae</uri>
          </mesh>
        </geometry>
      </visual>

      <collision name='collision'>
        <pose>0 -0.012 0 1.57 -0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.12</radius>
            <length>0.07</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0.1</mu>
              <mu2>0.1</mu2>
              <fdir1>0 0 1</fdir1>
              <slip1>0.0</slip1>
