<?xml version='1.0'?>
<!--value of mass for chassis is just an estimate, for tires it's taken from manufacturer-->
<!--values of inertia matrix for chassis is roughly estimated-->
<!--values of inertia matrices for wheels are taken from meshlab are based on simplified models-->
<!--value for friction coeff for wheels is selected to be between tire on rubber coeff and tire on grass coeff-->
<sdf version='1.7'>
  <model name='statek'>
    <static>false</static>
    <link name='chassis'>

      <collision name='collision'>
        <pose>0 0.04 0.02</pose>
        <geometry>
          <box>
            <size>.62 .9 .15</size>
          </box>
        </geometry>
      </collision>

      <visual name='visual'>
        <geometry>
          <mesh>
            <uri>model://statek/chassis.dae</uri>
          </mesh>
        </geometry>
      </visual>

      <inertial>
        <inertia>
          <ixx>0.97125</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.51916</iyy>
          <iyz>0</iyz>
          <izz>1.4379</izz>
        </inertia>
        <mass>14</mass>
        <pose>0 0.04 -0.02</pose>
      </inertial>

      <sensor type="ray" name="lidar">
        <always_on>true</always_on>
        <pose>-.03 0 .11 0 0 0</pose>
        <visualize>true</visualize>
        <update_rate>30</update_rate>
        <ray>
          <scan>
            <horizontal>
              <samples>625</samples>
              <resolution>0.5</resolution>
              <min_angle>-3.1415</min_angle>
              <max_angle>3.1415</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>.12</min>
            <max>10</max>
            <resolution>0.01</resolution>
          </range>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>1</stddev>
          </noise>
        </ray>
        <plugin name="sensor_plugin" filename="libgazebo_ros_laser.so">
          <topicName>scan</topicName>
          <frameName>laser_link</frameName>
        </plugin>
      </sensor>
    </link>

    <link name='left_back_wheel'>

      <pose>0.270332 0.253025 -0.031261 0 0 -1.5708</pose>

      <visual name='visual'>
        <geometry>
          <mesh>
            <uri>model://statek/wheel.dae</uri>
          </mesh>
        </geometry>
      </visual>

      <collision name='collision'>
        <pose>0 -0.012 0 -1.57 -0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.12</radius>
            <length>0.07</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0.1</mu>
              <mu2>0.1</mu2>
              <fdir1>0 0 1</fdir1>
              <slip1>0.0</slip1>
              <slip2>0.0</slip2>
            </ode>
          </friction>
          <contact>
            <ode>
              <min_depth>0.005</min_depth>
              <kp>1e8</kp>
            </ode>
          </contact>
        </surface>
      </collision>

      <inertial>
        <inertia>
          <ixx>0.00521083333</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.00936</iyy>
          <iyz>0</iyz>
          <izz>0.00521083333</izz>
        </inertia>
        <mass>1.3</mass>
      </inertial>
    </link>

    <joint type="revolute" name="left_back_wheel_hinge">
      <child>left_back_wheel</child>
      <parent>chassis</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-10000</lower>
          <upper>10000</upper>
        </limit>
      </axis>
    </joint>

    <link name='right_back_wheel'>

      <pose>-0.264143 0.253025 -0.031261 0 0 1.5708</pose>

      <visual name='visual'>
