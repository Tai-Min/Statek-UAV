          <ixz>0</ixz>
          <iyy>0.00936</iyy>
          <iyz>0</iyz>
          <izz>0.00521083333</izz>
        </inertia>
        <mass>1.3</mass>
      </inertial>

    </link>

    <joint type="revolute" name="right_back_wheel_hinge">
      <child>right_back_wheel</child>
      <parent>chassis</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-10000</lower>
          <upper>10000</upper>
        </limit>
      </axis>
    </joint>

