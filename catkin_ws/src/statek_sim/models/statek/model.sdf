      <inertial>
        <inertia>
          <ixx>0.00521083333</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.00936</iyy>
          <iyz>0</iyz>
          <izz>0.00521083333</izz>
        </inertia>
