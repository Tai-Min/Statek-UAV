          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0.1</mu>
              <mu2>0.1</mu2>
              <fdir1>0 0 1</fdir1>
              <slip1>0.0</slip1>
              <slip2>0.0</slip2>
            </ode>
          </friction>
          <contact>
            <ode>
              <min_depth>0.005</min_depth>
              <kp>1e8</kp>
            </ode>
          </contact>
        </surface>
      </collision>

      <inertial>
        <inertia>
          <ixx>0.00521083333</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.00936</iyy>
          <iyz>0</iyz>
          <izz>0.00521083333</izz>
        </inertia>
