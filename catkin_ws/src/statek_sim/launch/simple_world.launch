<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="statek_name" default="$(eval open(find('statek_config') + '/name').read())"/>

  <!-- Launch world -->
  <include file="$(find statek_sim)/launch/world.launch">
    <arg name="world_name" value="$(find statek_sim)/worlds/simple_world.world"/>
    <arg name="statek_name" value="$(arg statek_name)"/>
  </include>

</launch>