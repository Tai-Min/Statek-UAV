<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="statek_name" default="$(eval open(find('statek_config') + '/name').read())"/>

    <group ns="$(arg statek_name)/machine_learning/">
        <node name="lidar_fconv_dataset_collector" pkg="statek_ml" type="lidar_fconv_dataset_collector_node.py" output="screen" respawn="false">
            <param name="statek_name" value="$(arg statek_name)"/>
        </node>
    </group>

</launch>
