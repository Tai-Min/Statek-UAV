<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="statek_name" default="$(eval open(find('statek_config') + '/name').read())"/>

    <include file="$(find statek_map)/launch/gps_to_tf.launch">
        <arg name="statek_name" value="$(arg statek_name)"/>
    </include>

    <include file="$(find statek_map)/launch/local_mapper.launch">
        <arg name="statek_name" value="$(arg statek_name)"/>
    </include>

    <include file="$(find statek_map)/launch/voronoi_mapper.launch">
        <arg name="statek_name" value="$(arg statek_name)"/>
    </include>

</launch>
