<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="statek_name" default="$(eval open(find('statek_config') + '/name').read())"/>

    <group ns="$(arg statek_name)/map/">
        <node name="gps_to_tf" pkg="statek_map" type="gps_to_tf_node" output="screen" respawn="true">
            <param name="statek_name" value="$(arg statek_name)"/>
            <rosparam command="load" file="$(find statek_config)/yaml/earth_config.yaml"/>
        </node>
    </group>
</launch>
