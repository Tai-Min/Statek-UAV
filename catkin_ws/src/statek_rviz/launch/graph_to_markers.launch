<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="statek_name" default="$(eval open(find('statek_config') + '/name').read())"/>

    <group ns="$(arg statek_name)/map/">
        <node name="graph_to_markers" pkg="statek_rviz" type="graph_to_markers_node" output="screen" respawn="true">
            <param name="statek_name" value="$(arg statek_name)"/>
        </node>
    </group>

</launch>
